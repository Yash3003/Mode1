package com.hcl.ast;

import java.io.Serializable;

public class Student implements Serializable {
	int sno;
	String name;
	String city;
	transient double cgp;
	
	public Student(int sno, String name, String city, double basic) {
		super();
		this.sno = sno;
		this.name = name;
		this.city = city;
		this.cgp = basic;
	}

	@Override
	public String toString() {
		return "Student [sno=" + sno + ", name=" + name + ", city=" + city + ", basic=" + cgp + "]";
	}
	
	

}
